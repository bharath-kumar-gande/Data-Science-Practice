    "def Age_group(age):\n",
    "    if (age >=0 and age <=10):\n",
    "        return 'Child'\n",
    "    elif (age >10 and age <= 25):\n",
    "          return 'Young'\n",
    "    elif (age >25 and age <= 50):\n",
    "          return 'Middle'\n",
    "    else:\n",
    "        return 'Old'    "
   ]
